if result != -1:
    print(f'Target is found at index {result}')

else:
    print('Target not exist in a list')


def binarySearch(arr, target):
    low = 0
    high = len(arr) - 1

    while low <= high:
        mid = low + (high-low)//2

        if arr[mid] == target:
            return mid

        elif arr[mid] > target:
            high = mid -1

        else:
            low = mid + 1

    return -1

