   "source": [
    "def binarySearch(arr, l, r, x):\n",
    "    while l <= x:\n",
    "        mid = l + (r - l)//2\n",
    "\n",
    "        if arr[mid] == x:\n",
    "            return mid\n",
    "\n",
    "        elif arr[mid] < x:\n",
    "            l = mid + 1\n",
    "\n",
    "        else:\n",
    "            r = mid -1\n",
    "\n",
    "    return -1"
   ]
  }
